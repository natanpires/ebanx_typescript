{
  "name": "wiser-educacao-ebanx-library",
  "license": "BSD-3-Clause",
  "author": "Natan Pires de Souza <partisansouza@gmail.com> (https://wisereducacao.com/)",
  "version": "0.1.2",
  "description": "Migration of Ebanx NodeJs lib to Typescript and also upgraded.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/natanpires/ebanx_typescript"
  },
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register ./__mocks__/*.test.ts",
    "pretty": "pretty-quick",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "tslint": "tslint 'src/**/*.ts'",
    "pretslint": "yarn type-check",
    "tslint:fix": "tslint --fix 'src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@types/node": "^13.13.2",
    "@types/request": "^2.48.4",
    "@types/supertest": "^2.0.6",
    "husky": "^1.1.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "request": "^2.88.2",
    "ts-node": "^8.9.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-prettier": "^1.15.0"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/chai": "^4.2.11",
    "mocha": "^7.1.1",
    "chai": "^4.2.0",
    "typescript": "^3.4.5"
  },
  "keywords": [
    "ebanx",
    "wiser",
    "typescript"
  ],
  "files": [
    "lib/**/*"
  ]
}
